{
	//----------------------------------------------------
	// ワークベンチ
	//----------------------------------------------------
	"workbench": {
		"colorTheme": "Ayu Dark",
		"iconTheme": "material-icon-theme",
		"editor": {
			"showTabs": true,
			"highlightModifiedTabs": true,
			"tabSizing": "shrink",
			"tabCloseButton": "off"
		},
		"startupEditor": "off",
		"activityBar": {
			"visible": true
		}
	},
	//----------------------------------------------------
	// エディタ
	//----------------------------------------------------
	"editor": {
		"fontSize": 11,
		"fontFamily": "FiraCode Nerd Font Mono",
		"fontLigatures": true,
		"wordWrap": "on",
		"formatOnSave": false,
		"formatOnPaste": false,
		"suggestSelection": "first",
		"minimap": {
			"enabled": false
		},
		"tabSize": 4,
		"detectIndentation": false
	},
	//----------------------------------------------------
	// ターミナル
	//----------------------------------------------------
	"terminal.integrated": {
		"copyOnSelection": true,
		"fontSize": 10,
		"fontFamily": "FiraCode Nerd Font Mono"
	},
	//----------------------------------------------------
	// vim
	//----------------------------------------------------
	"vim": {
		"easymotion": true,
		"easymotionMarkerForegroundColorOneChar": "#00F0AA",
		"easymotionDimBackground": false,
		"easymotionMarkerWidthPerChar": 19,
		"easymotionMarkerHeight": 0,
		"easymotionMarkerFontFamiliy": "Cica",
		"easymotionMarkerFontSize": "12.5",
		"easymotionMarkerFontWeight": "bold",
		"easymotionKeys": "asdfhjklwqeruioopghty;",
		"easymotionMarkerYOffset": 13.5,
		"easymotionmarkerforegroundcoloronechar": "#da70d6",
		"incsearch": true,
		"usesystemclipboard": true,
		"useCtrlKeys": true,
		"hlsearch": true,
		"sorround": true,
		// vimキーバインド変更
		"insertModeKeyBindings": [
			{
				"before": [
					"j",
					"j"
				],
				"after": [
					"<Esc>"
				]
			}
		],
		"normalModeKeyBindingsNonRecursive": [
			{
				"before": [
					"<leader>",
					"d"
				],
				"after": [
					"d",
					"d"
				]
			},
			{
				"before": [
					"<C-n>"
				],
				"commands": [
					":nohl"
				]
			},
			{
				"before": [
					"s",
					"h"
				],
				"after": [
					"<C-w>",
					"h"
				]
			},
			{
				"before": [
					"s",
					"j"
				],
				"after": [
					"<C-w>",
					"j"
				]
			},
			{
				"before": [
					"s",
					"k"
				],
				"after": [
					"<C-w>",
					"k"
				]
			},
			{
				"before": [
					"s",
					"l"
				],
				"after": [
					"<C-w>",
					"l"
				]
			},
			{
				"before": [
					"g",
					"l"
				],
				"after": [
					"g",
					"t"
				]
			},
			{
				"before": [
					"g",
					"h"
				],
				"after": [
					"g",
					"T"
				]
			},
			{
				"before": [
					"]"
				],
				"commands": [
					{
						"command": "C_Cpp.PeekDeclaration"
					}
				],
				"when": [
					"editorLangId == c"
				]
			}, //宣言を見る c専用
			{
				"before": [
					"["
				],
				"commands": [
					{
						"command": "editor.action.peekDefinition"
					}
				]
			}, //定義を見る
			{
				"before": [
					"s",
					"s"
				],
				"commands": [
					":split"
				]
			}, //水平に開く
			{
				"before": [
					"s",
					"v"
				],
				"commands": [
					":vsplit"
				]
			}, //垂直に開く
			{
				"before": [
					"<Leader>",
					":"
				],
				"commands": [
					{
						"command": "workbench.action.showCommands"
					}
				]
			}, //コマンドパレット
			{
				"before": [
					"<Leader>",
					";"
				],
				"commands": [
					{
						"command": "workbench.action.quickOpen"
					}
				]
			} //ファイル検索
		],
		"leader": "<space>",
		// vimのステータスバー周りの設定
		"statusBarColorControl": false
	},
	//----------------------------------------------------
	// その他
	//----------------------------------------------------
	"files.trimTrailingWhitespace": true,
	"files.autoSave": "off",
	"search.searchOnType": false,
	"emmet": {
		"includeLanguages": {
			"erb": "html"
		}
	},
	//****************************************************
	// 拡張機能
	//****************************************************
	// spell checker
	"cSpell": {
		"enabledLanguageIds": [
			"css",
			"html",
			"javascript",
			"json",
			"less",
			"markdown",
			"plaintext",
			"scss",
			"text",
			"ruby",
			"yaml",
			"yml"
		]
	},
	// background-cover
	"backgroundCover": {
		"imagePath": "/Users/mejamoleo/Pictures/image_for_vscode/max-mckinnon-c9OCWLka764-unsplash.jpg",
		"randomImageFolder": "/Users/mejamoleo/Pictures/image_for_vscode",
		"opacity": 1
	},
	// vscode インテリコード
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	// beautify フォーマッター
	"beautify.language": {
		"js": {
			"type": [
				"javascript",
				"json",
				"jsonc"
			],
			"filename": [
				".jshintrc",
				".jsbeautifyrc"
			]
		},
		"css": [
			"css",
			"less",
			"scss"
		],
		"html": [
			"htm",
			"html",
			"erb"
		]
	},
	//----------------------------------------------------
	// 言語系
	//----------------------------------------------------
	"ruby.useBundler": true,
	"cSpell.userWords": [
		"CODEBAS",
		"Gemfile",
		"Micropost",
		"bootstrap",
		"bootstrap sprokets",
		"datetime",
		"downcase",
		"hexdigest",
		"laravel",
		"mailtrap",
		"microposts",
		"minitest",
		"mixins",
		"nonblank",
		"path",
		"phper",
		"postgresql",
		"railstutorial",
		"rubygems",
		"signup",
		"signup path",
		"smtp",
		"sprokets",
		"sqlite",
		"stylesheets",
		"tmpfs",
		"uneditable",
		"unfollow",
		"unfollowed",
		"upcase",
		"urlsafe",
		"vimindmeister"
	],
	"[json]": {
		"editor.defaultFormatter": "HookyQR.beautify"
	},
	"[html]": {
		"editor.defaultFormatter": "HookyQR.beautify"
	},
	"[javascript]": {
		"editor.defaultFormatter": "HookyQR.beautify"
	},
